  function renderTable(page) {
    const startIndex = (page - 1) * pageSize;
    const endIndex = Math.min(startIndex + pageSize, track3Data.length);
    const tableBody = document.getElementById('track3-table-body');
    if (!tableBody) return;
    tableBody.innerHTML = '';
    for (let i = startIndex; i < endIndex; i++) {
      const team = track3Data[i];
      const row = document.createElement('tr');
      row.innerHTML = `
        <td class="whitespace-nowrap py-4 pl-4 pr-3 text-sm font-medium text-gray-900 sm:pl-6">${team.name}</td>
