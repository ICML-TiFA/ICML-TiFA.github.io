  function padtrack1Data(track3Data) {
    const remainder = track3Data.length % pageSize;
    if (remainder !== 0) {
      const padCount = pageSize - remainder;
      for (let i = 0; i < padCount; i++) {
        track3Data.push({
          rank: '',
          country: '',
          institution: '',
          score: '',
          teamName: '',
          ch1Score: '',
          ch2Score: '',
          ch3Score: '',
